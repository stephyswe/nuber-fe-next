    "node_modules": true,
    "public": false,
    "README.md": true,
    "yarn.lock": true,
    ".gitignore": true,
    ".prettierignore": true,
    "next.config.js": false,
    "postcss.config.js": true,
    "tsconfig.json": true,
    "next-env.d.ts": true,
    "graphql.schema.json": true,
    ".husky": true,
    ".next": true,
    ".github": true,
    ".npmrc": true,
